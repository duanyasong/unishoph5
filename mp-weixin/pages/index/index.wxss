/***产品列表*****/
.md-prolist-a-hd{
	line-height: 36rpx;
	padding-left: 5rpx;
	font-weight: 600;
	font-size: 16rpx;
	color: #333;
	background-color: #fff;
	margin-bottom: 1rpx;
}
.md-prolist-a-more{
	float: right;
	color: #555;
	font-size: 14rpx;
	margin-right: 3rpx;
	font-weight: 400;
	cursor: pointer;
}
.md-prolist-a-more:after{
	content:"\E6A3";
	font-family: "iconfont" !important;
	display: inline-block;
	font-size: 14rpx;
	font-weight: 300;
	color: #999;
}
.md-prolist-a{
	margin-bottom: 5rpx;
}
.md-prolist-a:after{
	clear: both;
	content:".";
	visibility: hidden;
	display: block;
	height: 0;
}
.md-prolist-a-item{
	padding: 2rpx;
	
	margin-bottom: 5rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
	width: 50%;
	float: left;
	display: block;
	cursor: pointer;
	text-decoration: none;
}
.md-prolist-a-item-img{
	margin-bottom: 5rpx;
}
.md-prolist-a-img{
	width: 100%;
}
.md-prolist-a-row{
	padding-left: 3rpx;
	padding-bottom: 3rpx;
}
.md-prolist-a-title{	
	padding-left: 3rpx;
	margin-bottom: 5rpx;
	font-size: 14rpx;
	color: #444;
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	padding-left: 3rpx;
}
.md-prolist-a-price{
	color: #FF6600;
	font-size: 18rpx;
}
.md-prolist-a-pd{
	background-color: #fff;
}
